{
  "name": "Short-Content Timer",
  "description": "Timer",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "menu.html",
    "default_icon": "timer.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["scripts/content.js"],
      "run_at": "document_start"
    }
  ],
  "background": {
    "service_worker": "scripts/background.js"
  }
}


  